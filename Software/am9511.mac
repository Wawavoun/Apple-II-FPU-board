; AM9511.MAC
;
; DEFINE PORTS USED FOR AM9511 APU.
;
         .8080
;
         PUBLIC   SRIFPU,DRIFPU,RFPU,DWFPU,CWFPU
;
SLTADR   EQU  0E090H      ; APPLEII SLOT 1 
SRIFPU   EQU  SLTADR+0H   ; STATUS READ INIT ADR
DRIFPU   EQU  SLTADR+1H   ; DATA READ INIT ADR
RFPU     EQU  SLTADR+6H   ; READ ADR
DWFPU    EQU  SLTADR+6H   ; DATA WRITE ADR
CWFPU    EQU  SLTADR+7H   ; COMMAND WRITE ADR           
;
;
         END
